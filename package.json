  "version": "1.0.0",
  "description": "A calendar menubar app powered by electron",
  "dependencies": {
    "menubar": "*",
    "node-calendar": "~0.1.4",
    "gulp-eta": "~0.4.7",
    "gulp": "~3.9.0",
    "sassqwatch": "~0.7.4",
    "jquery": "~2.1.4",
